### Variables necesarias para docker-compose de movilidad-sostenible
# Copia este archivo como `.env` y ajusta los valores antes de ejecutar `docker compose up`

# Postgres
POSTGRES_ROOT_USER=admin
POSTGRES_ROOT_PASSWORD=changeme
POSTGRES_ROOT_DB=movilidad_sostenible

# DB usuarios por servicio (ejemplo, puedes usar los mismos valores para pruebas locales)
USUARIO_DB_USER=usuario_user
USUARIO_DB_PASSWORD=usuario_pass
ADMIN_DB_USER=admin_user
ADMIN_DB_PASSWORD=admin_pass
BICIS_DB_USER=bicis_user
BICIS_DB_PASSWORD=bicis_pass
VIAJE_DB_USER=viaje_user
VIAJE_DB_PASSWORD=viaje_pass
SLOTS_DB_USER=slots_user
SLOTS_DB_PASSWORD=slots_pass
ESTACIONES_DB_USER=estaciones_user
ESTACIONES_DB_PASSWORD=estaciones_pass
CIUDAD_DB_USER=ciudad_user
CIUDAD_DB_PASSWORD=ciudad_pass
FINES_DB_USER=fines_user
FINES_DB_PASSWORD=fines_pass
MANTENIMIENTO_DB_USER=maintenance_user
MANTENIMIENTO_DB_PASSWORD=maintenance_pass

# RabbitMQ
RABBITMQ_DEFAULT_USER=guest
RABBITMQ_DEFAULT_PASS=guest

# Stripe / otros (dejar vac√≠o si no los usas en local)
STRIPE_SECRET_KEY=
STRIPE_PUBLISHABLE_KEY=
STRIPE_WEBHOOK_SECRET=
